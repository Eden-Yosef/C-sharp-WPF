<Window x:Class="ProjectExams.endExam"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectExams"
        mc:Ignorable="d"
        Background="AliceBlue"
        Title="endExam" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <StackPanel Grid.Column="0" HorizontalAlignment="Center"  Margin="0 80 0 0">
            <TextBlock  Margin="0,9,0,5" HorizontalAlignment="Center" FontSize="25" FontWeight="Bold" Text="Successfully!" />
            <TextBlock FontSize="20"  FontWeight="SemiBold" HorizontalAlignment="Center" Margin="5" Text="Your final score is:"></TextBlock><Label x:Name="grade" FontSize="40"  FontWeight="SemiBold" Margin="5" HorizontalAlignment="Center"/>
        </StackPanel>
        
    </Grid>
</Window>
